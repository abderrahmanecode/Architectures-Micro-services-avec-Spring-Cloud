    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/keycloak-angular.mjs",
      "es2020": "./fesm2020/keycloak-angular.mjs",
      "es2015": "./fesm2015/keycloak-angular.mjs",
      "node": "./fesm2015/keycloak-angular.mjs",
      "default": "./fesm2020/keycloak-angular.mjs"
    }
  },
  "sideEffects": false
